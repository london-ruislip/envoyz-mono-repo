{
    "defaultSeverity": "error",
    "extends": "tslint:recommended",
    "jsRules": {},
    "rules": {
      "quotemark": [true, "single", "jsx-double"],
      "max-line-length": [true, 140],
      "interface-name": false,
      "object-literal-sort-keys": false,
      "ordered-imports": false,
      "ter-arrow-parens": false,
      "no-console": false,
      "array-type": false,
      "arrow-parens": false
    },
    "rulesDirectory": []
  }
  